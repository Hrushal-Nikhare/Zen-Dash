{
  "manifest_version": 3,
  "name": "Zenboard",
  "version": "0.1",
  "description": "Dashboard",
  "author": "Hrushal",
  "chrome_url_overrides": {
    "newtab": "startpage.html"
  },
  "chrome_settings_overrides": {
    "homepage": "startpage.html"
  },
  "permissions": [
    "tabs"
  ],
  "background": {
    "scripts": [
      "bg-script.js"
    ],
    "persistent": true,
    "type": "module"
  },
  "icons": {
    "16": "ico/16.png",
    "32": "ico/32.png",
    "48": "ico/48.png",
    "64": "ico/64.png",
    "128": "ico/128.png"
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self';"
  }
}